# If you come from bash you might have to change your $PATH.
# export PATH=$HOME/bin:/usr/local/bin:$PATH

# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/Users/hamz/opt/miniconda3/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/Users/hamz/opt/miniconda3/etc/profile.d/conda.sh" ]; then
        . "/Users/hamz/opt/miniconda3/etc/profile.d/conda.sh"
    else
        export PATH="/Users/hamz/opt/miniconda3/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<


# Path to your oh-my-zsh installation.
export ZSH="$HOME/.oh-my-zsh"

# My Themes
ZSH_THEME="robbyrussell"

# MY Plugins
plugins=(
    git
    macos
    python
    timer
    aliases
    gitignore
    thefuck
)

export QT_MAC_WANTS_LAYER=1
export PYTHONDONTWRITEBYTECODE=1

#Run oh-my-zsh
source $ZSH/oh-my-zsh.sh
